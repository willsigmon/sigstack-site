{
  "name": "Carter Intake â†’ Hot Lead Follow-up Scheduling",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "carter-hot-lead-followup",
        "options": { "responseMode": "responseNode" }
      },
      "name": "Hot Lead Webhook",
      "type": "n8n-nodes-base.webhook",
      "position": [250, 300],
      "webhookId": "carter-hot-lead-followup"
    },
    {
      "parameters": {
        "conditions": {
          "options": { "caseSensitive": true, "typeValidation": "strict" },
          "conditions": [
            {
              "leftValue": "={{ $json.body.event }}",
              "rightValue": "schedule_followup",
              "operator": { "type": "string", "operation": "equals" }
            }
          ]
        }
      },
      "name": "Is Followup Event?",
      "type": "n8n-nodes-base.if",
      "position": [470, 300]
    },
    {
      "parameters": {
        "values": {
          "string": [
            { "name": "referenceNumber", "value": "={{ $json.body.referenceNumber }}" },
            { "name": "clientName", "value": "={{ $json.body.client.name }}" },
            { "name": "clientEmail", "value": "={{ $json.body.client.email }}" },
            { "name": "clientPhone", "value": "={{ $json.body.client.phone }}" },
            { "name": "leadTier", "value": "={{ $json.body.leadTier }}" },
            { "name": "scheduledFor", "value": "={{ $json.body.scheduledFor }}" },
            { "name": "title", "value": "={{ $json.body.title }}" }
          ]
        }
      },
      "name": "Extract Data",
      "type": "n8n-nodes-base.set",
      "position": [690, 200]
    },
    {
      "parameters": {
        "calendar": "primary",
        "title": "ðŸ”¥ {{ $json.title }}",
        "start": "={{ $json.scheduledFor }}",
        "end": "={{ new Date(new Date($json.scheduledFor).getTime() + 30*60000).toISOString() }}",
        "additionalFields": {
          "description": "Hot Lead Follow-up\n\nReference: {{ $json.referenceNumber }}\nClient: {{ $json.clientName }}\nPhone: {{ $json.clientPhone }}\nEmail: {{ $json.clientEmail }}\n\nThis is an auto-scheduled follow-up for a hot lead.",
          "attendees": [],
          "reminders": {
            "useDefault": false,
            "overrides": [
              { "method": "popup", "minutes": 15 },
              { "method": "popup", "minutes": 5 }
            ]
          }
        }
      },
      "name": "Create Calendar Event",
      "type": "n8n-nodes-base.googleCalendar",
      "position": [910, 200],
      "disabled": true,
      "notesInFlow": true,
      "notes": "Enable and configure with Google Calendar OAuth"
    },
    {
      "parameters": {
        "fromEmail": "notifications@carterhelms.com",
        "toEmail": "carter.helms@highstreetins.com",
        "subject": "ðŸ”¥ HOT LEAD: Follow-up scheduled for {{ $json.clientName }}",
        "text": "A follow-up has been auto-scheduled for a HOT lead.\n\nClient: {{ $json.clientName }}\nScheduled: {{ $json.scheduledFor }}\n\nReference: {{ $json.referenceNumber }}\nPhone: {{ $json.clientPhone }}\nEmail: {{ $json.clientEmail }}\n\nThis lead scored high and should be contacted promptly!",
        "options": {}
      },
      "name": "Notify Carter",
      "type": "n8n-nodes-base.emailSend",
      "position": [910, 400],
      "disabled": true,
      "notesInFlow": true,
      "notes": "Enable and configure with SMTP credentials"
    },
    {
      "parameters": {
        "content": "## ðŸ”¥ Hot Lead Follow-up Scheduled\n\n**Client:** {{ $json.clientName }}\n**Reference:** {{ $json.referenceNumber }}\n**Scheduled:** {{ $json.scheduledFor }}\n\nðŸ“ž {{ $json.clientPhone }}\nðŸ“§ {{ $json.clientEmail }}"
      },
      "name": "Format Slack Message",
      "type": "n8n-nodes-base.markdown",
      "position": [1130, 200]
    },
    {
      "parameters": {
        "channel": "#carter-leads",
        "text": "={{ $json.data }}",
        "otherOptions": {}
      },
      "name": "Send to Slack",
      "type": "n8n-nodes-base.slack",
      "position": [1350, 200],
      "disabled": true,
      "notesInFlow": true,
      "notes": "Enable and configure with Slack credentials"
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { success: true, message: 'Follow-up scheduled', referenceNumber: $json.referenceNumber, scheduledFor: $json.scheduledFor } }}"
      },
      "name": "Return Success",
      "type": "n8n-nodes-base.respondToWebhook",
      "position": [1570, 300]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ { success: false, error: 'Invalid event type' } }}"
      },
      "name": "Return Error",
      "type": "n8n-nodes-base.respondToWebhook",
      "position": [690, 400]
    }
  ],
  "connections": {
    "Hot Lead Webhook": {
      "main": [[{ "node": "Is Followup Event?", "type": "main", "index": 0 }]]
    },
    "Is Followup Event?": {
      "main": [
        [{ "node": "Extract Data", "type": "main", "index": 0 }],
        [{ "node": "Return Error", "type": "main", "index": 0 }]
      ]
    },
    "Extract Data": {
      "main": [
        [
          { "node": "Create Calendar Event", "type": "main", "index": 0 },
          { "node": "Notify Carter", "type": "main", "index": 0 }
        ]
      ]
    },
    "Create Calendar Event": {
      "main": [[{ "node": "Format Slack Message", "type": "main", "index": 0 }]]
    },
    "Notify Carter": {
      "main": [[{ "node": "Format Slack Message", "type": "main", "index": 0 }]]
    },
    "Format Slack Message": {
      "main": [[{ "node": "Send to Slack", "type": "main", "index": 0 }]]
    },
    "Send to Slack": {
      "main": [[{ "node": "Return Success", "type": "main", "index": 0 }]]
    }
  },
  "settings": { "executionOrder": "v1" },
  "staticData": null,
  "pinData": {},
  "tags": [{ "name": "carter" }, { "name": "insurance" }, { "name": "hot-leads" }]
}
